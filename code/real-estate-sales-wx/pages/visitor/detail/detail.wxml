<form class="form" bindsubmit="formSubmit" report-submit='true'>
	<view class="form-body">
		<input class='hidden' name="id" type="text" value='{{id}}'></input>
		<input class='hidden' name="createrid" type="text" value='{{model.createrid}}'></input>
		<input class='hidden' name="realEstateId" type="text" value='{{realEstateId}}'></input>
		<view class="form-item">
			<view class="form-lable">选择楼盘</view>
			<input disabled class='form-input' name="realEstateName" disabled placeholder="请选择楼盘" type="text" value='{{realEstateName}}' bindtap="toSelect"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">客户姓名</view>
			<input class='form-input' name="customer" placeholder="请输入姓名" type="text" value='{{customer}}'></input>
		</view>
		<view class="form-item">
			<view class="form-lable">联系方式</view>
			<input class='form-input' name="phone" placeholder="请输入联系方式" type="number" value='{{phone}}'></input>
		</view>
		<view class="form-item">
			<view class="form-lable">所属团队</view>
			<input disabled class='form-input' name="team" placeholder="请选择所属团队" type="text" value='{{team}}' bindtap="getTeam"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">置业顾问</view>
			<input class='form-input' name="adviser" placeholder="请输入置业顾问" type="text" value='{{adviser}}'></input>
		</view>
		<view class="form-item">
			<view class="form-lable">居住区域</view>
			<input disabled class='form-input' name="address" placeholder="请选择居住区域" type="text" value='{{address}}' bindtap="getAddress"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">客户职业</view>
			<input disabled class='form-input' name="occupation" placeholder="请选择客户职业" type="text" value='{{occupation}}' bindtap="getOccupation"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">到访次数</view>
			<input disabled class='form-input' name="times" placeholder="请选择到访次数" type="text" value='{{times}}' bindtap="getTimes"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">购买用途</view>
			<input disabled class='form-input' name="purpose" placeholder="请选择购买用途" type="text" value='{{purpose}}' bindtap="getPurpose"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">付款方式</view>
			<input disabled class='form-input' name="payment" placeholder="请选择付款方式" type="text" value='{{payment}}' bindtap="getPayment"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">意向产品</view>
			<input disabled class='form-input' name="productType" placeholder="请选择意向产品" type="text" value='{{productType}}' bindtap="getProductType"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">意向面积</view>
			<input disabled class='form-input' name="area" placeholder="请选择意向面积" type="text" value='{{area}}' bindtap="getArea"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">意向等级</view>
			<input disabled class='form-input' name="level" placeholder="请选择意向等级" type="text" value='{{level}}' bindtap="getLevel"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">购买意向</view>
			<input disabled class='form-input' name="intention" placeholder="请选择购买意向" type="text" value='{{intention}}' bindtap="getIntention"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">未成交原因</view>
			<input class='form-input' name="nodeal" placeholder="请输入未成交原因" type="text" value='{{nodeal}}'></input>
		</view>
		<view class="form-item">
			<view class="form-lable">报备人属性</view>
			<input disabled class='form-input' name="property" placeholder="请选择报备人属性" type="text" value='{{property}}' bindtap="getProperty"></input>
			<text class="iconfont icon-more form-input-select"></text>
		</view>
		<view class="form-item">
			<view class="form-lable">报备人姓名</view>
			<input class='form-input' name="reporter" placeholder="请输入报备人姓名" type="text" value='{{reporter}}'></input>
		</view>
		<view>
			<view class="textarea-label">备注</view>
			<textarea class="textarea-text" placeholder="请输入备注" name="remark" value="{{remark}}"></textarea>
		</view>
	</view>
</form>